{
  "manifest_version": 1,
  "name":             "Help TA!",
  "version":          "1.0",
  "description":      "Students can request help from TAs by clicking on a button",
  "icons":            {
    "16":  "img/icon16.png",
    "48":  "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js":      ["myscript.js"],
      "run_at":  "document_end"
    }
  ]
}
